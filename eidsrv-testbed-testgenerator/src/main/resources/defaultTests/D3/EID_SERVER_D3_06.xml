<?xml version="1.0" encoding="UTF-8"?>
<p:testcaseDefinition xmlns:p="http://www.secunet.com"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.secunet.com ../../xmlScheme/sn_TestcaseDefinition.xsd ">
	<p:name>EID_SERVER_D3_06</p:name>
	<p:description>Positive test verifying that the eIDAS-Interface of the eID-Server supports the Encrypt-then-MAC extension for TLS.</p:description>
	<p:modulePath>
		<p:node>D</p:node>
		<p:node>3</p:node>
	</p:modulePath>
	<p:profiles>
		<p:profile>EIDAS_MW</p:profile>
		<p:profile>CRYPTO</p:profile>
		<p:profile>TLS_ETM</p:profile>
	</p:profiles>
	<p:eservice>ERSA</p:eservice><p:clientExplanation>Configure the server to accept the certificate chain for client E. This includes the TLS, XML signature and the CV certificate.</p:clientExplanation>
	<p:certificateBaseNames>
		<p:certificate>CERT_EID_TLS_EIDSERVER_1</p:certificate>
	</p:certificateBaseNames>
	<p:steps>
		<p:step>OUT_TLS_ATTACHED</p:step>
	</p:steps>
	<p:variables>
		<p:variable name="TLS_VERSION">TLSv12</p:variable>
		<p:variable name="TLS_SELECTED_VERSION">TLSv12</p:variable>
		<p:variable name="TLS_CIPHER_SUITES">TLS_ECDHE_RSA_WITH_AES_128_CBC_SHA</p:variable>
		<p:variable name="TLS_SELECTED_CIPHER_SUITE">TLS_ECDHE_RSA_WITH_AES_128_CBC_SHA</p:variable>
		<p:variable name="TLS_ALERT_LEVEL_RECEIVED">0</p:variable>
		<p:variable name="TLS_ENCRYPT_THEN_MAC_EXTENSION">true</p:variable>
		<p:variable name="TLS_ENCRYPT_THEN_MAC_EXTENSION_ENABLED">true</p:variable>
	</p:variables>
</p:testcaseDefinition>