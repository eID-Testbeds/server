<?xml version="1.0" encoding="UTF-8"?>
<p:testcaseDefinition xmlns:p="http://www.secunet.com"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.secunet.com ../xmlScheme/sn_TestcaseDefinition.xsd ">
	<p:name>EID_SERVER_C2_1_05</p:name>
	<p:description>Positive test verifying that the eID-Server supports the Encrypt-then-MAC extension.</p:description>
	<p:modulePath>
		<p:node>C</p:node>
		<p:node>2</p:node>
		<p:node>1</p:node>
	</p:modulePath>
	<p:profiles>
		<p:profile>PAOS</p:profile>
		<p:profile>EAC</p:profile>
		<p:profile>CRYPTO</p:profile>
		<p:profile>TLS_PSK</p:profile>
		<p:profile>TLS_ETM</p:profile>
	</p:profiles>
	<p:eservice>A</p:eservice>
	<p:certificateBaseNames>
		<p:certificate>CERT_EID_TLS_EIDSERVER_1</p:certificate>
	</p:certificateBaseNames>
	<p:steps>
		<p:step>OUT_USEID_REQUEST</p:step>
		<p:step>IN_USEID_RESPONSE</p:step>
		<p:step>OUT_TLS_ECARDAPI</p:step>
	</p:steps>
	<p:variables>
		<p:variable name="TLS_VERSION">TLSv12</p:variable>
		<p:variable name="TLS_SELECTED_VERSION">TLSv12</p:variable>
		<p:variable name="TLS_CIPHER_SUITES">TLS_RSA_PSK_WITH_AES_256_CBC_SHA</p:variable>
		<p:variable name="TLS_SELECTED_CIPHER_SUITE">TLS_RSA_PSK_WITH_AES_256_CBC_SHA</p:variable>
		<p:variable name="TLS_ELLIPTIC_CURVES">secp256r1,secp384r1</p:variable>
		<p:variable name="TLS_SUPPORTED_SIGNATURE_ALGORITHMS">SHA1withRSA</p:variable>
		<p:variable name="TLS_ALERT_LEVEL_RECEIVED">0</p:variable>
		<p:variable name="TLS_ENCRYPT_THEN_MAC_EXTENSION">true</p:variable>
		<p:variable name="TLS_ENCRYPT_THEN_MAC_EXTENSION_ENABLED">true</p:variable>
	</p:variables>
	
</p:testcaseDefinition>