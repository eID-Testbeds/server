	<p:name>EID_SERVER_C1_2_09</p:name>
	<p:description>Positive test checking the behaviour of the eID-Server during the PAOS communication in case the document is on the revocation list.</p:description>
	<p:modulePath>
		<p:node>C</p:node>
		<p:node>1</p:node>
		<p:node>2</p:node>
	</p:modulePath>
	<p:profiles>
		<p:profile>EAC</p:profile>
		<p:profile>PAOS</p:profile><p:profile>SOAP</p:profile>
		<p:profile>REVOKED_CARD</p:profile>
	</p:profiles>
	<p:eservice>B</p:eservice><p:clientExplanation>Configure the server to accept the certificate chain for client B. This includes the TLS, XML signature and the CV certificate.</p:clientExplanation>
	<p:eidcard>EIDCARD_8</p:eidcard>
	<p:certificateBaseNames>
		<p:certificate>CERT_EID_TLS_EIDSERVER_1</p:certificate>
	</p:certificateBaseNames>
