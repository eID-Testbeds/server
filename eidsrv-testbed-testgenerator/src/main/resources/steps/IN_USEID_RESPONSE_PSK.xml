		<p:value></p:value>
		<p:isMandatory>true</p:isMandatory>
	</p:ProtocolStepToken>
	<p:ProtocolStepToken>
		<p:name>ID</p:name>
		<p:parentName>PSK</p:parentName>
		<p:value>[PSK_ID]</p:value>
		<p:isMandatory>true</p:isMandatory>
	</p:ProtocolStepToken>
	<p:ProtocolStepToken>
		<p:name>Key</p:name>
		<p:parentName>PSK</p:parentName>
		<p:value>[PSK_KEY]</p:value>
		<p:isMandatory>true</p:isMandatory>
	</p:ProtocolStepToken>
	<p:ProtocolStepToken>
		<p:name>ResultMajor</p:name>
		<p:value>http://www.bsi.bund.de/ecard/api/1.1/resultmajor#ok</p:value>
		<p:isMandatory>true</p:isMandatory>
	</p:ProtocolStepToken>
	<p:ToSave parentName="Session" placeholder="ESERVICE_SESSION">ID</p:ToSave>
	<p:ToSave placeholder="PSK">Key</p:ToSave>
	<p:ToSave parentName="PSK" placeholder="SESSIONIDENTIFIER">ID</p:ToSave>
