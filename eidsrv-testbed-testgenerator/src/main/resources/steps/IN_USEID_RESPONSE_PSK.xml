	<!-- HTTP Tokens -->
	<p:HttpStepToken>
		<p:name>HTTP_STATUS_CODE</p:name>
		<p:value>HTTP/1.1 200 OK</p:value>
		<p:isMandatory>true</p:isMandatory>
	</p:HttpStepToken>
	<p:HttpStepToken>
		<p:name>Content-Type</p:name>
		<p:value>text/xml;charset=utf-8</p:value>
		<p:isMandatory>true</p:isMandatory>
	</p:HttpStepToken>
	<!-- Protocol tokens -->
	<p:ProtocolStepToken>
		<p:name>Session</p:name>
		<p:value></p:value>
		<p:isMandatory>true</p:isMandatory>
	</p:ProtocolStepToken>
	<p:ProtocolStepToken>
		<p:name>ID</p:name>
		<p:parentName>Session</p:parentName>
		<p:value></p:value>
		<p:isMandatory>true</p:isMandatory>
	</p:ProtocolStepToken>
	<p:ProtocolStepToken>
