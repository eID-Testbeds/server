<?xml version="1.0" encoding="UTF-8"?>
<p:Step xmlns:p="http://www.secunet.com"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.secunet.com ../xmlScheme/sn_TestcaseDefinition.xsd" >
	<p:TargetInterface>eID-Interface</p:TargetInterface>
	<!-- Schema name -->
	<!--<p:Schema>PROVIDE_XSD</p:Schema> -->
	<!-- HTTP Tokens -->
	<p:HttpStepToken>
		<p:name>HTTP_STATUS_CODE</p:name>
		<p:value>HTTP/1.1 200 OK</p:value>
		<p:isMandatory>true</p:isMandatory>
	</p:HttpStepToken>
	<p:HttpStepToken>
		<p:name>Content-Type</p:name>
		<p:value>text/xml;charset=utf-8</p:value>
		<p:isMandatory>true</p:isMandatory>
	</p:HttpStepToken>
	<!-- Protocol tokens -->
	<p:ProtocolStepToken>
		<p:name>ResultMajor</p:name>
		<p:value>http://www.bsi.bund.de/ecard/api/1.1/resultmajor#ok</p:value>
		<p:isMandatory>true</p:isMandatory>
	</p:ProtocolStepToken>
	<p:ProtocolStepToken>
		<p:name>getResultResponse</p:name>
		<p:value></p:value>
		<p:isMandatory>true</p:isMandatory>
	</p:ProtocolStepToken>
	<p:ProtocolStepToken>
		<p:name>PersonalData</p:name>
		<p:value></p:value>
		<p:isMandatory>true</p:isMandatory>
	</p:ProtocolStepToken>
	<p:ProtocolStepToken>
		<p:name>OperationsAllowedByUser</p:name>
		<p:value></p:value>
		<p:isMandatory>true</p:isMandatory>
	</p:ProtocolStepToken>
	
	<!-- The message must also contain the element "OperationsAllowedByUser" which marks [ATTRIBUTE_A] as ALLOWED and the remaining attributes as PROHIBITED. -->
	<p:ProtocolStepToken>
		<p:name>DocumentType</p:name>
		<p:parentName>OperationsAllowedByUser</p:parentName>
		<p:value>[OABU_DOCUMENTTYPE]</p:value>
		<p:isMandatory>true</p:isMandatory>
	</p:ProtocolStepToken>
	<p:ProtocolStepToken>
		<p:name>IssuingState</p:name>
		<p:parentName>OperationsAllowedByUser</p:parentName>
		<p:value>[OABU_ISSUINGSTATE]</p:value>
		<p:isMandatory>true</p:isMandatory>
	</p:ProtocolStepToken>
	<p:ProtocolStepToken>
		<p:name>DateOfExpiry</p:name>
		<p:parentName>OperationsAllowedByUser</p:parentName>
		<p:value>[OABU_DATEOFEXPIRY]</p:value>
		<p:isMandatory>true</p:isMandatory>
	</p:ProtocolStepToken>
	<p:ProtocolStepToken>
		<p:name>GivenNames</p:name>
		<p:parentName>OperationsAllowedByUser</p:parentName>
		<p:value>[OABU_GIVENNAMES]</p:value>
		<p:isMandatory>true</p:isMandatory>
	</p:ProtocolStepToken>
	<p:ProtocolStepToken>
		<p:name>FamilyNames</p:name>
		<p:parentName>OperationsAllowedByUser</p:parentName>
		<p:value>[OABU_FAMILYNAMES]</p:value>
		<p:isMandatory>true</p:isMandatory>
	</p:ProtocolStepToken>
	<p:ProtocolStepToken>
		<p:name>ArtisticName</p:name>
		<p:parentName>OperationsAllowedByUser</p:parentName>
		<p:value>[OABU_ARTISTICNAME]</p:value>
		<p:isMandatory>true</p:isMandatory>
	</p:ProtocolStepToken>
	<p:ProtocolStepToken>
		<p:name>AcademicTitle</p:name>
		<p:parentName>OperationsAllowedByUser</p:parentName>
		<p:value>[OABU_ACADEMICTITLE]</p:value>
		<p:isMandatory>true</p:isMandatory>
	</p:ProtocolStepToken>
	<p:ProtocolStepToken>
		<p:name>DateOfBirth</p:name>
		<p:parentName>OperationsAllowedByUser</p:parentName>
		<p:value>[OABU_DATEOFBIRTH]</p:value>
		<p:isMandatory>true</p:isMandatory>
	</p:ProtocolStepToken>
	<p:ProtocolStepToken>
		<p:name>PlaceOfBirth</p:name>
		<p:parentName>OperationsAllowedByUser</p:parentName>
		<p:value>[OABU_PLACEOFBIRTH]</p:value>
		<p:isMandatory>true</p:isMandatory>
	</p:ProtocolStepToken>
	<p:ProtocolStepToken>
		<p:name>Nationality</p:name>
		<p:parentName>OperationsAllowedByUser</p:parentName>
		<p:value>[OABU_NATIONALITY]</p:value>
		<p:isMandatory>true</p:isMandatory>
	</p:ProtocolStepToken>
	<p:ProtocolStepToken>
		<p:name>BirthName</p:name>
		<p:parentName>OperationsAllowedByUser</p:parentName>
		<p:value>[OABU_BIRTHNAME]</p:value>
		<p:isMandatory>true</p:isMandatory>
	</p:ProtocolStepToken>
	<p:ProtocolStepToken>
		<p:name>PlaceOfResidence</p:name>
		<p:parentName>OperationsAllowedByUser</p:parentName>
		<p:value>[OABU_PLACEOFRESIDENCE]</p:value>
		<p:isMandatory>true</p:isMandatory>
	</p:ProtocolStepToken>
	<p:ProtocolStepToken>
		<p:name>ResidencePermitI</p:name>
		<p:parentName>OperationsAllowedByUser</p:parentName>
		<p:value>[OABU_RESIDENCEPERMITI]</p:value>
		<p:isMandatory>true</p:isMandatory>
	</p:ProtocolStepToken>
	<p:ProtocolStepToken>
		<p:name>RestrictedID</p:name>
		<p:parentName>OperationsAllowedByUser</p:parentName>
		<p:value>[OABU_RESTRICTEDID]</p:value>
		<p:isMandatory>true</p:isMandatory>
	</p:ProtocolStepToken>
	<p:ProtocolStepToken>
		<p:name>AgeVerification</p:name>
		<p:value>PROHIBITED</p:value>
		<p:isMandatory>true</p:isMandatory>
	</p:ProtocolStepToken>
	<p:ProtocolStepToken>
		<p:name>PlaceVerification</p:name>
		<p:value>PROHIBITED</p:value>
		<p:isMandatory>true</p:isMandatory>
	</p:ProtocolStepToken>
	
	<!-- the values for personal data -->
	<p:ProtocolStepToken>
		<p:name>DocumentType</p:name>
		<p:parentName>PersonalData</p:parentName>
		<p:value>[PERSONAL_DOCUMENTTYPE]</p:value>
		<p:isMandatory>false</p:isMandatory>
	</p:ProtocolStepToken>
	<p:ProtocolStepToken>
		<p:name>IssuingState</p:name>
		<p:parentName>PersonalData</p:parentName>
		<p:value>[PERSONAL_ISSUINGSTATE]</p:value>
		<p:isMandatory>false</p:isMandatory>
	</p:ProtocolStepToken>
	<p:ProtocolStepToken>
		<p:name>DateOfExpiry</p:name>
		<p:parentName>PersonalData</p:parentName>
		<p:value>[PERSONAL_DATEOFEXPIRY]</p:value>
		<p:isMandatory>false</p:isMandatory>
	</p:ProtocolStepToken>
	<p:ProtocolStepToken>
		<p:name>GivenNames</p:name>
		<p:parentName>PersonalData</p:parentName>
		<p:value>[PERSONAL_GIVENNAMES]</p:value>
		<p:isMandatory>false</p:isMandatory>
	</p:ProtocolStepToken>
	<p:ProtocolStepToken>
		<p:name>FamilyNames</p:name>
		<p:parentName>PersonalData</p:parentName>
		<p:value>[PERSONAL_FAMILYNAMES]</p:value>
		<p:isMandatory>false</p:isMandatory>
	</p:ProtocolStepToken>
	<p:ProtocolStepToken>
		<p:name>ArtisticName</p:name>
		<p:parentName>PersonalData</p:parentName>
		<p:value>[PERSONAL_ARTISTICNAME]</p:value>
		<p:isMandatory>false</p:isMandatory>
	</p:ProtocolStepToken>
	<p:ProtocolStepToken>
		<p:name>AcademicTitle</p:name>
		<p:parentName>PersonalData</p:parentName>
		<p:value>[PERSONAL_ACADEMICTITLE]</p:value>
		<p:isMandatory>false</p:isMandatory>
	</p:ProtocolStepToken>
	<p:ProtocolStepToken>
		<p:name>DateString</p:name>
		<p:parentName>DateOfBirth</p:parentName>
		<p:value>[PERSONAL_DATEOFBIRTH]</p:value>
		<p:isMandatory>false</p:isMandatory>
	</p:ProtocolStepToken>
	<p:ProtocolStepToken>
		<p:name>FreetextPlace</p:name>
		<p:parentName>PlaceOfBirth</p:parentName>
		<p:value>[PERSONAL_PLACEOFBIRTH]</p:value>
		<p:isMandatory>false</p:isMandatory>
	</p:ProtocolStepToken>
	<p:ProtocolStepToken>
		<p:name>Nationality</p:name>
		<p:parentName>PersonalData</p:parentName>
		<p:value>[PERSONAL_NATIONALITY]</p:value>
		<p:isMandatory>false</p:isMandatory>
	</p:ProtocolStepToken>
	<p:ProtocolStepToken>
		<p:name>BirthName</p:name>
		<p:parentName>PersonalData</p:parentName>
		<p:value>[PERSONAL_BIRTHNAME]</p:value>
		<p:isMandatory>false</p:isMandatory>
	</p:ProtocolStepToken>
	<p:ProtocolStepToken>
		<p:name>PlaceOfResidence</p:name>
		<p:parentName>PersonalData</p:parentName>
		<p:value>[PERSONAL_PLACEOFRESIDENCE]</p:value>
		<p:isMandatory>false</p:isMandatory>
	</p:ProtocolStepToken>
	<p:ProtocolStepToken>
		<p:name>ResidencePermitI</p:name>
		<p:parentName>PersonalData</p:parentName>
		<p:value>[PERSONAL_RESIDENCEPERMITI]</p:value>
		<p:isMandatory>false</p:isMandatory>
	</p:ProtocolStepToken>
	<p:ProtocolStepToken>
		<p:name>ID</p:name>
		<p:parentName>RestrictedID</p:parentName>
		<p:value>[PERSONAL_RESTRICTEDID]</p:value>
		<p:isMandatory>false</p:isMandatory>
	</p:ProtocolStepToken>
	
</p:Step>