<?xml version="1.0" encoding="UTF-8"?>
<p:Step xmlns:p="http://www.secunet.com"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.secunet.com ../xmlScheme/sn_TestcaseDefinition.xsd" >
	<p:TargetInterface>SAML</p:TargetInterface>
	<!-- Schema name
	<p:Schema>bsi_TCToken.xsd</p:Schema> -->
	<!-- HTTP Tokens -->
	<p:HttpStepToken>
		<p:name>HTTP_STATUS_CODE</p:name>
		<p:value>HTTP/1.1 200 OK</p:value>
		<p:isMandatory>true</p:isMandatory>
	</p:HttpStepToken>
	<p:HttpStepToken>
		<p:name>Content-Type</p:name>
		<p:value>text/xml;charset=utf-8</p:value>
		<p:isMandatory>true</p:isMandatory>
	</p:HttpStepToken>
	<!-- Protocol tokens -->
	<p:ProtocolStepToken>
		<p:name>TCTokenType</p:name>
		<p:value></p:value>
		<p:isMandatory>true</p:isMandatory>
	</p:ProtocolStepToken>
	<p:ProtocolStepToken>
		<p:name>ServerAddress</p:name>
		<p:value></p:value>
		<p:isMandatory>true</p:isMandatory>
	</p:ProtocolStepToken>
	<p:ProtocolStepToken>
		<p:name>CommunicationErrorAddress</p:name>
		<p:value>[COMMUNICATIONERRORADDRESS]</p:value>
		<p:isMandatory>false</p:isMandatory>
	</p:ProtocolStepToken>
	<p:ProtocolStepToken>
		<p:name>Binding</p:name>
		<p:value>urn:liberty:paos:2006-08</p:value>
		<p:isMandatory>true</p:isMandatory>
	</p:ProtocolStepToken>
	<!-- Values that have to be saved -->
	<p:ToSave>SessionIdentifier</p:ToSave>
	<p:ToSave>RefreshAddress</p:ToSave>
	<p:ToSave>ServerAddress</p:ToSave>
</p:Step>